# Description:
# - This script is used to install the required source code for running massprospecting master nodes.
# Parameters:
# - $1: github_username
# - $2: github_password
# 

RUN if [ ! -d $5 ]; then
    git clone https://$3:$4@github.com/$1 $5
fi

RUN git -C $5 fetch --all

RUN git -C $5 reset --hard origin/$3
